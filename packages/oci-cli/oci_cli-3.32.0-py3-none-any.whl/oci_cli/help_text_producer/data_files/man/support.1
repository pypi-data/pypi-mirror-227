.sp
.nf
.ft C
    export compartment_id=<substitute\-value\-of\-compartment_id> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/support/user/user/create.html#cmdoption\-compartment\-id
    export country=<substitute\-value\-of\-country> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/support/user/user/create.html#cmdoption\-country
    export csi=<substitute\-value\-of\-csi> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/support/user/user/create.html#cmdoption\-csi
    export first_name=<substitute\-value\-of\-first_name> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/support/user/user/create.html#cmdoption\-first\-name
    export last_name=<substitute\-value\-of\-last_name> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/support/user/user/create.html#cmdoption\-last\-name
    export ocid=<substitute\-value\-of\-ocid> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/support/user/user/create.html#cmdoption\-ocid
    export organization_name=<substitute\-value\-of\-organization_name> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/support/user/user/create.html#cmdoption\-organization\-name
    export phone=<substitute\-value\-of\-phone> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/support/user/user/create.html#cmdoption\-phone
    export timezone=<substitute\-value\-of\-timezone> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/support/user/user/create.html#cmdoption\-timezone

    oci support user user create \-\-compartment\-id $compartment_id \-\-country $country \-\-csi $csi \-\-first\-name $first_name \-\-last\-name $last_name \-\-ocid $ocid \-\-organization\-name $organization_name \-\-phone $phone \-\-timezone $timezone
.ft P
.fi
