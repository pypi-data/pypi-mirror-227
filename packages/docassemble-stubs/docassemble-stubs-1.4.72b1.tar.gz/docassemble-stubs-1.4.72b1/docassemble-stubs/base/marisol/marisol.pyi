class GenericTextOverlay:
    text: Incomplete
    area: Incomplete
    def __init__(self, text, area) -> None: ...
    def apply(self, c) -> None: ...
    def position(self, c): ...

class BatesOverlay(GenericTextOverlay): ...
class StaticOverlay(GenericTextOverlay): ...

